        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewStaggeredLayoutFragment2"
            app:destination="@id/recyclerviewStaggeredLayoutFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewViewTypeFragment"
            app:destination="@id/recyclerviewViewTypeFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewHorizontalLayoutFragment"
            app:destination="@id/recyclerviewHorizontalLayoutFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewSwipeToRefreshFragment"
            app:destination="@id/recyclerviewSwipeToRefreshFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewRadioButtonFragment"
            app:destination="@id/recyclerviewRadioButtonFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewCheckboxFragment"
            app:destination="@id/recyclerviewCheckboxFragment" />
