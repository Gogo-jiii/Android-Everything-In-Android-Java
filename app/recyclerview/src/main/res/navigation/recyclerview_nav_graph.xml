        android:label="Recyclerview Drag Drop Item"
        tools:layout="@layout/fragment_recyclerview_drag_drop_item" />
    <fragment
        android:id="@+id/recyclerviewGridLayoutFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewGridLayoutFragment"
        android:label="Recyclerview Grid Layout"
        tools:layout="@layout/fragment_recyclerview_grid_layout" />
    <fragment
        android:id="@+id/recyclerviewStaggeredLayoutFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewStaggeredLayoutFragment"
