            android:id="@+id/action_recyclerviewFragment_to_multipleItemsSelectionRecyclerviewFragment"
            app:destination="@id/multipleItemsSelectionRecyclerviewFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewSwipeToDeleteFragment"
            app:destination="@id/recyclerviewSwipeToDeleteFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewSwipeToDeleteIconFragment"
            app:destination="@id/recyclerviewSwipeToDeleteIconFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewDragDropItemFragment"
            app:destination="@id/recyclerviewDragDropItemFragment" />
        <action
            android:id="@+id/action_recyclerviewFragment_to_recyclerviewGridLayoutFragment"
            app:destination="@id/recyclerviewGridLayoutFragment" />
