        tools:layout="@layout/fragment_recyclerview_staggered_layout" />
    <fragment
        android:id="@+id/recyclerviewViewTypeFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewViewTypeFragment"
        android:label="Recyclerview View Type"
        tools:layout="@layout/fragment_recyclerview_view_type" />
    <fragment
        android:id="@+id/recyclerviewHorizontalLayoutFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewHorizontalLayoutFragment"
        android:label="Recyclerview Horizontal Layout"
        tools:layout="@layout/fragment_recyclerview_horizontal_layout" />
    <fragment
        android:id="@+id/recyclerviewSwipeToRefreshFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewSwipeToRefreshFragment"
        android:label="Recyclerview Swipe To Refresh"
        tools:layout="@layout/fragment_recyclerview_swipe_to_refresh" />
    <fragment
        android:id="@+id/recyclerviewRadioButtonFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewRadioButtonFragment"
        android:label="Recyclerview Radio Button"
        tools:layout="@layout/fragment_recyclerview_radio_button" />
    <fragment
        android:id="@+id/recyclerviewCheckboxFragment"
        android:name="com.example.recyclerview.fragments.RecyclerviewCheckboxFragment"
        android:label="Recyclerview Checkbox"
        tools:layout="@layout/fragment_recyclerview_checkbox" />
</navigation>