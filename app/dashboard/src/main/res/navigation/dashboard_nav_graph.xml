<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_nav_graph"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.dashboard.DashboardFragment"
        android:label="Everything About Android Java"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_logs_nav_graph"
            app:destination="@id/logs_nav_graph" />
        <action
            android:id="@+id/action_dashboardFragment_to_toast_nav_graph"
            app:destination="@id/toast_nav_graph" />
        <action
            android:id="@+id/action_dashboardFragment_to_button_nav_graph"
            app:destination="@id/button_nav_graph" />
    </fragment>

    <include app:graph="@navigation/logs_nav_graph" />
    <include app:graph="@navigation/toast_nav_graph" />
    <include app:graph="@navigation/button_nav_graph" />
</navigation>